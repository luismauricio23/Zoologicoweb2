@model Zoologico.Models.Cliente
@{
    ViewBag.Title = "Registro";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<link href="~/Library/style/main.css" rel="stylesheet" />
<link href="~/Library/style/Registro.css" rel="stylesheet" />
<link href="~/Library/style/SocialRedes.css" rel="stylesheet" />

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div id="carousel1" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="~/Library/Imagenes/rino.jpg" alt="">
                <div class="carousel-caption d-none d-md-block">
                    <h5 class="animated bounceInRight" style="animation-delay: 1s">First slide label</h5>
                    <p class="animated bounceInLeft" style="animation-delay: 2s">Nulla vitae elit libero, a pharetra augue mollis interdum.</p>

                </div>
            </div>
            <div class="carousel-item">
                <img src="~/Library/Imagenes/cohala.jpg" alt="">
                <div class="carousel-caption d-none d-md-block">
                    <h5 class="animated slideInDown" style="animation-delay: 1s">secon slide label</h5>
                    <p class="animated fadeInUp" style="animation-delay: 2s">Nulla vitae elit libero, a pharetra augue mollis interdum.</p>

                </div>
            </div>
            <div class="carousel-item">
                <img src="~/Library/Imagenes/chita.jpg" alt="">
                <div class="carousel-caption d-none d-md-block">
                    <h5 class="animated zoomIn" style="animation-delay: 1s">three slide label</h5>
                    <p class="animated fadeInleft" style="animation-delay: 2s">Nulla vitae elit libero, a pharetra augue mollis interdum.</p>

                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#carousel1" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carousel1" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
        <ol class="carousel-indicators">
            <li data-target="#carousel1" data-slide-to="0" class="active"></li>
            <li data-target="#carousel1" data-slide-to="1"></li>
            <li data-target="#carousel1" data-slide-to="2"></li>
        </ol>
    </div>

    <br>
    <br>
    <form class="form-group">

        <h2 class="titulo">Formulario Registro</h2>

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-row">
            <div class="form-group col-md-5 m">
                <h5 class="color">Cedula</h5>
                @Html.EditorFor(model => model.Cedula_Cliente, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Cedula_Cliente, "", new { @class = "text-danger" })
            </div>
            <div class="form-group col-md-5 m">
                <h5 class="color">Nombre</h5>
                @Html.EditorFor(model => model.Nombre_Cliente, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Nombre_Cliente, "", new { @class = "text-danger" })
            </div>
            <div class="form-group col-md-5 m">
                <h5 class="color">Apellido</h5>
                @Html.EditorFor(model => model.Apellido_Cliente, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Apellido_Cliente, "", new { @class = "text-danger" })
            </div>
            <div class="form-group col-md-5 m">
                <h5 class="color">Direccion</h5>
                @Html.EditorFor(model => model.Direccion_Cliente, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Direccion_Cliente, "", new { @class = "text-danger" })
            </div>
            <div class="form-group col-md-5 m">
                <h5 class="color">Telefono</h5>
                @Html.EditorFor(model => model.Telefono_Cliente, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Telefono_Cliente, "", new { @class = "text-danger" })
            </div>
            <div class="form-group col-md-5 m">
                <h5 class="color">Edad</h5>
                @Html.EditorFor(model => model.Edad_Cliente, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Edad_Cliente, "", new { @class = "text-danger" })
            </div>
            <div class="form-group col-md-10 m">
                <h5 class="color">Contraseña</h5>
                @Html.EditorFor(model => model.Pass_Cliente, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Pass_Cliente, "", new { @class = "text-danger" })
            </div>


            <div class="form-group">
                <div class="col-md-offset-1 col-md-10">
                    <input type="submit" value="Guardar" class="btn btn-outline-dark" />
                </div>
            </div>
        </div>
    </form>


}
